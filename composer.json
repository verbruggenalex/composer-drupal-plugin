{
  "name": "verbruggenalex/robo-drupal",
  "type": "robo-tasks",
  "description": "Robo Drupal",
  "authors": [
    {
      "name": "Alex Verbruggen",
      "email": "verbruggenalex@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "consolidation/robo": "^2.2",
    "cweagans/composer-patches": "^1.6",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.8",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpro/grumphp": "^1.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "phpro/grumphp": [
        "https://patch-diff.githubusercontent.com/raw/phpro/grumphp/pull/818.patch"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "VerbruggenAlex\\RoboDrupal\\": "src/"
    }
  }
}
